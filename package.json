{
  "name": "pta3",
  "version": "2.0.0",
  "description": "CSS compiler for the pta system",
  "scripts": {
    "build": "sass src/scss/pta3.scss css/pta3.css --style=expanded --no-source-map",
    "watch": "sass src/scss/pta3.scss css/pta3.css --style=expanded --source-map --watch",
    "prepack": "del \"../_packages/pta3.zip\"",
    "pack": "\"E:/Software/7-Zip/7z.exe\" a -tzip ../_packages/pta3.zip -x!src/ -x!*.md -x!*.git* -x!*.lock -x!assets/"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "author": "Asacolips",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "help": "^3.0.2",
    "install": "^0.13.0",
    "sass": "^1.53.0",
    "typescript": "^5.7.3"
  }
}
